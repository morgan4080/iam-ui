{
  "name": "prestaui",
  "version": "0.0.0",
  "scripts": {
    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "rebuild": "npm run clean && npm install && npm run build",
    "dev-build": "vue-tsc --noEmit && vite build --base=/admin/ --mode local",
    "dev": "vite --base=/admin/ --port 9099",
    "build": "vue-tsc --noEmit && vite build --base=/admin/",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx,.vue --fix",
    "deploy": "vue-tsc --noEmit && vite build --base=/admin/",
    "serve": "vite preview",
    "production": "vite --base=/admin/ --host 0.0.0.0 --port 80  --mode production",
    "prettier": "prettier --write src"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.11",
    "@heroicons/vue": "^2.0.16",
    "@popperjs/core": "^2.11.6",
    "@vuelidate/core": "^2.0.0",
    "@vuelidate/validators": "^2.0.0",
    "@vueuse/core": "^9.13.0",
    "axios": "^1.3.4",
    "libphonenumber-js": "^1.10.21",
    "nprogress": "^0.2.0",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "^18.14.5",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/compiler-sfc": "^3.2.47",
    "@vue/eslint-config-typescript": "^11.0.2",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "install": "^0.13.0",
    "postcss": "^8.4.21",
    "prettier": "2.8.4",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^1.2.0"
  }
}
