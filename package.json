{
  "name": "prestaui",
  "version": "0.0.0",
  "scripts": {
    "production": "vue-tsc --noEmit && vite build --base=/admin/ --mode production",
    "staging": "vue-tsc --noEmit && vite build --base=/admin/ --mode staging",
    "development": "vue-tsc --noEmit && vite build --base=/admin/ --mode development",
    "dev": "vite --base=/admin/ --port 9099",
    "build": "vue-tsc --noEmit && vite build --base=/admin/",
    "rebuild": "npm run clean && npm install && npm run build",
    "serve": "vite preview",
    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx,.vue --fix",
    "prettier": "prettier --write src"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.11",
    "@heroicons/vue": "^2.0.16",
    "@iconify/vue": "^4.1.1",
    "@popperjs/core": "^2.11.6",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.3",
    "@vueuse/core": "^10.1.0",
    "axios": "^1.4.0",
    "libphonenumber-js": "^1.10.21",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.23",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "vuetify": "^3.3.10"
  },
  "devDependencies": {
    "@mdi/font": "7.0.96",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
